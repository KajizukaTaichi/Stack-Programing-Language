( #FizzBuzzを求める#
    ((FizzBuzz) print )
    i 15 mod 0 equal if

    ( 
        ((Fizz) print )
        i 3 mod 0 equal if 

        (
            ((Buzz) print )
            i 5 mod 0 equal if 

            (i print )
            i 5 mod 0 equal not if
        )
        i 3 mod 0 equal not if
    ) #条件分岐にelse文がないのでnot演算を用いる#
    i 15 mod 0 equal not if 
)
(fizzbuzz) var

1 (i) var
( fizzbuzz eval
    i 1 add (i) var
) #ループを100回繰り返す#
100 repeat eval
